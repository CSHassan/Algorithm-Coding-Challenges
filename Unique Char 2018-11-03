 /*       Unique
         * returns true if all the char in the array is unique
         * Date:2018-11-03
         * Time:around 25 mins
         * Question: make a function to check if all the char in the string is unique.
         * Type:Easy
        */

 
 public static bool Unique(char[] text)
        {
            if (IsUnique(text) == true)
            {
                Console.WriteLine("Not UNIQUE");
                return false;
            }
            else
                Console.WriteLine("all uniques");
                return true;
           
        }
        public static bool IsUnique(char[] text)
        {
            int n = text.Length;
            for(int j=0;j<n;j++)
                for(int i=j+1; i < n; i++)
                {
                    if(text[i] == text[j])
                    {
                        Console.WriteLine("I: "+text[i]+" J: "+text[j]);
                        return true;
                    }
                   
                }

            return false;
        }
